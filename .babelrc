{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "corejs": 3
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript",
    "babel-preset-gatsby"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "@babel/plugin-transform-runtime",
    "babel-plugin-styled-components",
    "lodash",
    [
      "module-resolver",
      {
        "root": ["./src", "./static", "./test"],
        "extensions": [".js", ".json", ".png", ".ts", ".tsx"]
      }
    ]
  ]
}
